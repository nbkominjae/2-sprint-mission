    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "multer": "^2.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "superstruct": "^2.0.2"
